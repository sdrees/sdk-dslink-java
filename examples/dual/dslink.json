{
  "name": "dual",
  "version": "0.0.1",
  "description": "An example link to demonstrate a requester and responder",
  "license": "Apache",
  "author": {
    "name": "Samuel Grenier",
    "email": "samrg472@gmail.com"
  },
  "main": "bin/dual",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/sdk-dslink-java"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/sdk-dslink-java/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "dual"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "org.dsa.iot.dual.Main"
    },
    "valuePersistenceEnabled": {
      "type": "bool",
      "value": true
    },
    "qosPersistenceEnabled": {
      "type": "bool",
      "value": false
    }
  }
}
